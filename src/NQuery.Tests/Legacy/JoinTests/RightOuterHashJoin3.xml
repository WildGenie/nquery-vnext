		e1.FirstName + ' ' + e1.LastName AS FullName,
		MONTH(e1.BirthDate) AS BirthDateMonth,
		e2.EmployeeID,
		e2.FirstName + ' ' + e2.LastName AS FullName,
		MONTH(e2.BirthDate) AS BirthDateMonth
